                "${workspaceFolder}/main/**",
                "${workspaceFolder}/components/**",
                "${IDF_PATH}/components/**",
                ""
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "path": [
                    "${workspaceFolder}/main",
                    "${workspaceFolder}/components",
                    "${IDF_PATH}/components",
